                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </JFXTextField>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" prefHeight="58.0" prefWidth="300.0">
               <children>
                  <Label text="Creation Date" />
                  <JFXDatePicker fx:id="cdate" prefHeight="28.0" prefWidth="257.0">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </JFXDatePicker>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" prefHeight="58.0" prefWidth="300.0">
               <children>
                  <Label text="President" />
