<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="650.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Elit.gui.OneClubController">
   <children>
      <ImageView fx:id="img" fitHeight="83.0" fitWidth="217.0" layoutX="90.0" layoutY="376.0" pickOnBounds="true" preserveRatio="true" />
