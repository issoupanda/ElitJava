                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </JFXComboBox>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
      <Button fx:id="edit" layoutX="454.0" layoutY="437.0" mnemonicParsing="false" onAction="#editClub" styleClass="dark-blue" stylesheets="@accueilelit.css" text="Edit" />
      <Button fx:id="delete" layoutX="454.0" layoutY="505.0" mnemonicParsing="false" onAction="#deleteClub" styleClass="dark-blue" stylesheets="@accueilelit.css" text="Delete" />
      <JFXTextField fx:id="picture" layoutX="85.0" layoutY="478.0" prefHeight="26.0" prefWidth="226.0" />
      <Button fx:id="picPic" layoutX="142.0" layoutY="533.0" mnemonicParsing="false" onAction="#changePic" styleClass="dark-blue" stylesheets="@accueilelit.css" text="Select a picture" />
